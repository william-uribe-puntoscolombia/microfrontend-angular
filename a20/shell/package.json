{
  "name": "shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "build:development": "ng build --configuration development-nfe",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "prepare": "husky",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format:css": "prettier --write \"src/**/*.css\"",
    "test": "vitest",
    "test:ui": "vitest --coverage --ui",
    "test:nowatch": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:cov": "vitest --coverage",
    "test:cov:nowatch": "ng test --no-watch --code-coverage  --browsers=ChromeHeadless",
    "test:cov:include": "ng test --code-coverage --browsers=ChromeHeadless --include"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^19.1.0",
    "@angular/animations": "^20.0.3",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/core": "^20.0.3",
    "@angular/forms": "^20.0.3",
    "@angular/platform-browser": "^20.0.3",
    "@angular/router": "^20.0.3",
    "@jsverse/transloco": "^7.6.1",
    "@ngrx/signals": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "@softarc/native-federation-node": "^2.0.28",
    "@softarc/native-federation-runtime": "^3.2.0",
    "es-module-shims": "^1.10.1",
    "ngx-permissions": "^19.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.17.1",
    "@angular-architects/native-federation": "^20.0.2",
    "@angular-devkit/build-angular": "^20.0.2",
    "@angular/build": "^20.0.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.3",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "angular-eslint": "20.1.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.34.0",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "engines": {
    "bun": "1.2.16"
  }
}
